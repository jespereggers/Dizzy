config_version=4

_global_script_classes=[ {
"base": "Area2D",
"class": "Player",
"language": "GDScript",
"path": "res://Scripts/player.gd"
}, {
"base": "Node2D",
"class": "Sensor",
"language": "GDScript",
"path": "res://Scripts/Sensor.gd"
} ]
_global_script_class_icons={
"Player": "",
"Sensor": ""
}

[application]

